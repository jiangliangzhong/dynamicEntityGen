<assInfo url="jdbc:mysql://192.168.50.248:3306/student?serverTimezone=UTC&amp;useUnicode=true&amp;characterEncoding=utf-8" userName="root" password="123456" driverClassName="com.mysql.jdbc.Driver"><!-- 存放数据库连接信息  -->
    <classFileDir>D:\MyJava\dynamicEntityGen\class-file</classFileDir>    <!--  任务生成的类文件存放位置 -->
    <tables>
        <!-- autoMatch:如果是自动生成，则fieldName标签无效,默认false -->
        <!-- tableName:不能缺失，表名-->
        <!-- className：不能缺失，完整的类名，如'lang.student'-->
        <!-- schema：模式名，可缺失，-->
        <table tableName="apply_school" className="apply_School" schema="" databaseName="chain" autoMatch="true">
            <!-- columnName: 不能缺失，数据库表字段名 -->
            <!-- property:对应的类字段名，可缺失-->
            <!-- jdbcType: jdbctype，可缺失-->
            <!-- javaType: javaType，可缺失-->

            <!-- moodifierNum: 描述符， 可缺失，默认为private-->
            <field columnName="id"/>
            <field columnName="id_card"/>
            <field columnName="name"/>
            <field columnName="qual_review"/>
        </table>
    </tables>
</assInfo>